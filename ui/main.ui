      /* Base colors and variables */
QMainWindow {
    background-color: #111827;
    color: #F9FAFB;
}

/* Tab styling */
QTabWidget::pane {
    border: none;
    background-color: #111827;
    border-radius: 6px;
}

QTabBar::tab {
    background-color: #1F2937;
    color: #F9FAFB;
    padding: 8px 16px;
    margin-right: 2px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    font-weight: 500;
}

QTabBar::tab:selected {
    background-color: #374151;
    border-bottom: 2px solid #10B981;
}

QTabBar::tab:hover:!selected {
    background-color: #2D3748;
}

/* LCD styling */
QLCDNumber {
    background-color: #1F2937;
    color: #10B981;
    border: none;
    border-radius: 4px;
}

/* Label styling */
QLabel {
    background-color: transparent;
    color: #F9FAFB;
    padding: 6px;
    font-weight: 400;
}

/* Header labels */
QLabel[objectName*=&quot;Header&quot;] {
    background-color: #374151;
    color: white;
    padding: 8px;
    border-radius: 4px;
    font-weight: 600;
}

/* Button styling - Fixed contrast issues */
QPushButton {
    background-color: #3B82F6;
    color: #FFFFFF;
    border-radius: 4px;
    padding: 10px 14px;
    font-weight: 600;
    font-size: 14px;
    letter-spacing: 0.5px;
    border: none;
}

QPushButton:hover {
    background-color: #2563EB;
}

QPushButton:pressed {
    background-color: #1D4ED8;
}

QPushButton:disabled {
    background-color: #374151;
    color: #9CA3AF;
}

/* Text components */
QTextEdit, QTextBrowser {
    background-color: #1F2937;
    color: #F9FAFB;
    border: none;
    border-radius: 4px;
    padding: 6px;
    selection-background-color: #4F46E5;
}

/* Menu styling */
QMenuBar {
    background-color: #1F2937;
    color: #F9FAFB;
    border-bottom: 1px solid #374151;
}

QMenuBar::item {
    background-color: transparent;
    padding: 6px 8px;
    border-radius: 2px;
}

QMenuBar::item:selected {
    background-color: #374151;
}

QMenu {
    background-color: #1F2937;
    color: #F9FAFB;
    border: 1px solid #111827;
    border-radius: 4px;
}

QMenu::item {
    padding: 6px 16px;
    border-radius: 2px;
}

QMenu::item:selected {
    background-color: #374151;
}

/* Scrollbar styling */
QScrollBar:vertical {
    border: none;
    background: #1F2937;
    width: 6px;
    border-radius: 3px;
}

QScrollBar::handle:vertical {
    background: #4F46E5;
    min-height: 24px;
    border-radius: 3px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar:horizontal {
    border: none;
    background: #1F2937;
    height: 6px;
    border-radius: 3px;
}

QScrollBar::handle:horizontal {
    background: #4F46E5;
    min-width: 24px;
    border-radius: 3px;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    width: 0px;
}

/* Triangle indicators */
QLabel[class=&quot;up-triangle&quot;] {
    color: #10B981; /* Green color for up movement */
    font-family: Arial;
    font-size: 16px;
}

QLabel[class=&quot;down-triangle&quot;] {
    color: #EF4444; /* Red color for down movement */
    font-family: Arial;
    font-size: 16px;
}

